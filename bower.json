{
  "name": "declarativewidgets-explorer",
  "version": "0.2.2",
  "description": "Represents a widget that uses visualizations to explore an urth-core-dataframe",
  "main": "urth-viz-explorer.html",
  "authors": [
    "Jupyter Community"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jupyter-incubator/declarativewidgets_explorer"
  },
  "keywords": [
    "urth",
    "visualization",
    "web-components"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "polymer": "Polymer/polymer#^1.2.4",
    "iron-collapse": "PolymerElements/iron-collapse",
    "iron-icon": "PolymerElements/iron-icon",
    "iron-resizable-behavior": "PolymerElements/iron-resizable-behavior#^1.0.0",
    "paper-button": "PolymerElements/paper-button",
    "paper-dialog": "PolymerElements/paper-dialog",
    "paper-dropdown-menu": "PolymerElements/paper-dropdown-menu",
    "paper-header-panel": "PolymerElements/paper-header-panel",
    "paper-icon-button": "PolymerElements/paper-icon-button",
    "paper-listbox": "PolymerElements/paper-listbox",
    "paper-input": "PolymerElements/paper-input",
    "paper-item": "PolymerElements/paper-item",
    "paper-menu": "PolymerElements/paper-menu",
    "paper-menu-button": "PolymerElements/paper-menu-button",
    "paper-radio-group": "PolymerElements/paper-radio-group",
    "paper-radio-button": "PolymerElements/paper-radio-button",
    "paper-toolbar": "PolymerElements/paper-toolbar",
    "vega-embed": "latest",
    "requirejs": "^2.1.0"
  },
  "devDependencies": {
    "webcomponentsjs": "^0.7.2",
    "web-component-tester": "*",
    "test-fixture": "PolymerElements/test-fixture#^1.0.0",
    "iron-component-page": "PolymerElements/iron-component-page#^1.0.0"
  },
  "private": true
}
